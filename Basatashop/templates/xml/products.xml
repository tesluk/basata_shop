{% load static %}
<products group_id="{{ group.id }}" group_name="{{ group.name }}" type_id="{{ type.id }}" type_name="{{ type.name }}">
	{% for prod in products %}
	<product id="{{ prod.id }}" name="{{ prod.name }}" price="{{ prod.price }}">
		<pic>{{ prod.picture_l }}</pic>
	</product>	
	{% endfor %}
</products>